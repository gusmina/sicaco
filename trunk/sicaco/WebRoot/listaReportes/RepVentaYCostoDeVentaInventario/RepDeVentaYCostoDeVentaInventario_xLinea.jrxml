	<parameter name="FECHA_INI" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_FIN" isForPrompting="true" class="java.util.Date"/>
	<parameter name="SUCURSAL" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="NOMBRE_SUCURSAL" isForPrompting="true" class="java.lang.String"/>
	<parameter name="LINEA" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT distinct
     inv_lin_linea.`LIN_NOMBRE` AS LINEA,
     inv_art_articulo.`ART_CODIGO` AS ART_CODIGO,
     inv_art_articulo.`ART_NOMBRE` AS ART_DESCRIPCION,
     inv_art_articulo.`MED_ID` AS UNIDAD,
    SUM(IF(fac_dfa_detalle_factura.`CNV_ID`= -2,
	 	fac_dfa_detalle_factura.`DFA_CANTIDAD`,
		IF(fac_dfa_detalle_factura.`CNV_ID`= inv_cnv_conversion.`CNV_ID`,
		fac_dfa_detalle_factura.`DFA_CANTIDAD` *
		inv_cnv_conversion.`CNV_FACTOR`,0)
  	 )) AS CANTIDAD_PRODUCTO,
    inv_art_articulo.`ART_PRECIO_MINIMO` AS PRECIO_COSTO,
    IF(inv_art_articulo.`ART_EXCENTO_IVA` = 1,1,0) AS COSTO_EXENTO,
    IF(inv_art_articulo.`ART_EXCENTO_IVA` = 1,0,1) AS COSTO_GRAVADO,
    inv_art_articulo.`ART_PRECIO_SUGERIDO` AS PRECIO_VENTA,
    SUM(IF(fac_dfa_detalle_factura.`DFA_EXENTO` = 1,fac_dfa_detalle_factura.`DFA_PRECIO_TOTAL`,0)) AS VENTA_EXENTO,
    SUM(IF(fac_dfa_detalle_factura.`DFA_EXENTO` = 0,fac_dfa_detalle_factura.`DFA_PRECIO_TOTAL` *100 / (con_rim_retencion_impuesto.`RIM_IMPUESTO`+100),0)) AS VENTA_BRUTA,
    SUM(IF(fac_dfa_detalle_factura.`DFA_EXENTO` = 0,fac_dfa_detalle_factura.`DFA_PRECIO_TOTAL`,0)) AS VENTA_GRAVADA

FROM
     `inv_art_articulo` inv_art_articulo INNER JOIN `fac_dfa_detalle_factura` fac_dfa_detalle_factura ON inv_art_articulo.`ART_CODIGO` = fac_dfa_detalle_factura.`ART_CODIGO`
     INNER JOIN `fac_fen_factura_encabezado` fac_fen_factura_encabezado ON fac_dfa_detalle_factura.`FEN_ID` = fac_fen_factura_encabezado.`FEN_ID`
     INNER JOIN `con_rim_retencion_impuesto` con_rim_retencion_impuesto ON fac_fen_factura_encabezado.`RIM_ID` = con_rim_retencion_impuesto.`RIM_ID`
	INNER JOIN `inv_lin_linea` inv_lin_linea ON inv_art_articulo.`LIN_ID` = inv_lin_linea.`LIN_ID`,
     `inv_cnv_conversion` inv_cnv_conversion

WHERE 
      fac_dfa_detalle_factura.`DFA_ID` IN (
                      SELECT DISTINCT    fac_dfa_detalle_factura.`DFA_ID` AS ID_DET
                      FROM
                         `inv_art_articulo` inv_art_articulo INNER JOIN `fac_dfa_detalle_factura` fac_dfa_detalle_factura ON inv_art_articulo.`ART_CODIGO` = fac_dfa_detalle_factura.`ART_CODIGO`
                         INNER JOIN `fac_fen_factura_encabezado` fac_fen_factura_encabezado ON fac_dfa_detalle_factura.`FEN_ID` = fac_fen_factura_encabezado.`FEN_ID`
                         INNER JOIN `inv_lin_linea` inv_lin_linea ON inv_art_articulo.`LIN_ID` = inv_lin_linea.`LIN_ID`,
                         `ctr_rfc_repositorio_facturas` ctr_rfc_repositorio_facturas
                    WHERE
                        fac_fen_factura_encabezado.`FEN_SERIE_FACTURA` <> '0000000000'
                        AND ctr_rfc_repositorio_facturas.`CFC_SERIE`= fac_fen_factura_encabezado.`FEN_SERIE_FACTURA`
                    	  AND (fac_fen_factura_encabezado.`FEN_NUMERO_FACTURA` BETWEEN
                        ctr_rfc_repositorio_facturas.`RFC_CORR_INI` AND ctr_rfc_repositorio_facturas.`RFC_CORR_FIN`)
	                      AND (IF($P{SUCURSAL}=-1,1,ctr_rfc_repositorio_facturas.`SUC_ID`= $P{SUCURSAL}))
                        AND DATE(fac_fen_factura_encabezado.`FEN_FECHA_FACTURA`) BETWEEN
	                          $P{FECHA_INI} AND $P{FECHA_FIN} and inv_cnv_conversion.`CNV_ID`=fac_dfa_detalle_factura.`CNV_ID`
			AND inv_lin_linea.`LIN_ID`=$P{LINEA}
      )
GROUP BY LINEA,ART_CODIGO
ORDER BY LINEA,ART_CODIGO ASC]]></queryString>

