	<queryString><![CDATA[SELECT
     fac_fen_factura_encabezado.`FEN_FECHA_FACTURA` AS FECHA_FACTURA,
     fac_fen_factura_encabezado.`FEN_NUMERO_FACTURA` AS NUMERO_COMPROBANTE,
     fac_fen_factura_encabezado.`FEN_REGISTRO` AS NUM_REGISTRO,
     inv_pro_proveedor.`PRO_NOMBRE` AS PRO_NOMBRE,
     IF (fac_fen_factura_encabezado.`FUS_ID`=1 AND fac_fen_factura_encabezado.`FEN_TOTAL_VENTAS_EXENTAS` IS NOT NULL,ROUND(fac_fen_factura_encabezado.`FEN_TOTAL_VENTAS_EXENTAS`,2),0.00) AS COMPRAS_EXENTAS_INTERNAS,
     IF (fac_fen_factura_encabezado.`FUS_ID`=3 AND fac_fen_factura_encabezado.`FEN_TOTAL_VENTAS_EXENTAS` IS NOT NULL,ROUND(fac_fen_factura_encabezado.`FEN_TOTAL_VENTAS_EXENTAS`,2),0.00) AS COMPRAS_EXENTAS_IMPORTAC,
     IF (fac_fen_factura_encabezado.`FUS_ID`=1 AND fac_fen_factura_encabezado.`FEN_TOTAL_VENTA` IS NOT NULL,ROUND(fac_fen_factura_encabezado.`FEN_TOTAL_VENTA`,2),0.00) AS COMPRAS_GRAVADAS_INTERNAS,
     IF (fac_fen_factura_encabezado.`FUS_ID`=3 AND fac_fen_factura_encabezado.`FEN_TOTAL_VENTA` IS NOT NULL,ROUND(fac_fen_factura_encabezado.`FEN_TOTAL_VENTA`,2),0.00) AS COMPRAS_GRAVADAS_IMPORTAC,
     IF(fac_fen_factura_encabezado.`FEN_IVA_RETENIDO` IS NOT NULL,ROUND(fac_fen_factura_encabezado.`FEN_IVA_RETENIDO`,2),0.00) AS IVA_RETENIDO,
     ROUND(
     (IF(fac_fen_factura_encabezado.`FEN_TOTAL_VENTAS_EXENTAS` IS NOT NULL,fac_fen_factura_encabezado.`FEN_TOTAL_VENTAS_EXENTAS`,0.00) +
      IF(fac_fen_factura_encabezado.`FEN_IVA_RETENIDO` IS NOT NULL,fac_fen_factura_encabezado.`FEN_IVA_RETENIDO`,0.00) +
      IF(fac_fen_factura_encabezado.`FEN_TOTAL_VENTA` IS NOT NULL, fac_fen_factura_encabezado.`FEN_TOTAL_VENTA`,0.00)),2) AS SUMATORIA_TOTAL

FROM
     `inv_pro_proveedor` inv_pro_proveedor INNER JOIN `fac_fen_factura_encabezado` fac_fen_factura_encabezado ON inv_pro_proveedor.`PRO_ID` = fac_fen_factura_encabezado.`PRO_ID`,
     `inv_cpr_contacto_proveedor` inv_cpr_contacto_proveedor

WHERE  MONTH(fac_fen_factura_encabezado.`FEN_FECHA_FACTURA`)=(SELECT CASE $P{MES}
                                                                              WHEN 'ENERO' THEN 1
                                                                              WHEN 'FEBRERO' THEN 2
                                                                              WHEN 'MARZO' THEN 3
                                                                              WHEN 'ABRIL' THEN 4
                                                                              WHEN 'MAYO' THEN 5
                                                                              WHEN 'JUNIO' THEN 6
                                                                              WHEN 'JULIO' THEN 7
                                                                              WHEN 'AGOSTO' THEN 8
                                                                              WHEN 'SEPTIEMBRE' THEN 9
                                                                              WHEN 'OCTUBRE' THEN 10
                                                                              WHEN 'NOVIEMBRE' THEN 11
                                                                              WHEN 'DICIEMBRE' THEN 12
                                                                              END)
       AND YEAR(fac_fen_factura_encabezado.`FEN_FECHA_FACTURA` )= CAST($P{AÑO} AS UNSIGNED)
ORDER BY FECHA_FACTURA,NUMERO_COMPROBANTE,NUM_REGISTRO]]></queryString>

