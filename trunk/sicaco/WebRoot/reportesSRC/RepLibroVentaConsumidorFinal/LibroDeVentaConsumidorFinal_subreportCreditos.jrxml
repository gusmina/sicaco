      (SELECT CASE $P{MES}
WHEN 'ENERO' THEN 1
WHEN 'FEBRERO' THEN 2
WHEN 'MARZO' THEN 3
WHEN 'ABRIL' THEN 4
WHEN 'MAYO' THEN 5
WHEN 'JUNIO' THEN 6
WHEN 'JULIO' THEN 7
WHEN 'AGOSTO' THEN 8
WHEN 'SEPTIEMBRE' THEN 9
WHEN 'OCTUBRE' THEN 10
WHEN 'NOVIEMBRE' THEN 11
WHEN 'DICIEMBRE' THEN 12
END)
      AND YEAR(fac_fen_factura_encabezado.`FEN_FECHA_FACTURA`)=CONVERT($P{Aﾃ前},SIGNED)
      AND fac_fen_factura_encabezado.`FEN_TIPO_FACTURA`='C'

UNION

