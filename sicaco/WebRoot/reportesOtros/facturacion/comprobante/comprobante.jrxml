	<variable name="saldoAnterior" class="java.lang.Double">
		<variableExpression><![CDATA[new Double($F{nuevoSaldo}.doubleValue() - ( ($F{txaMonto}.doubleValue() - $F{intereses}.doubleValue())*$F{factor}.intValue() ))]]></variableExpression>
